<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <size android:width="133dp" android:height="133dp" />
            <gradient android:angle="270" android:endColor="#b5b5b5" android:startColor="#cacaca" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <size android:width="133dp" android:height="133dp" />
            <gradient android:angle="270" android:endColor="#cacaca" android:startColor="#dddddd" />
        </shape>
    </item>
</selector>
