<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ConnectionStatusView">
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TypefaceTextView">
        <attr name="fontName">
            <enum name="DosisRegular" value="0" />
            <enum name="DosisSemiBold" value="1" />
            <enum name="DosisMedium" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="BinaryTextureView">
        <attr name="backgroundColor" format="color" />
    </declare-styleable>
</resources>
