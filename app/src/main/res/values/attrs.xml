<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ConnectionStatusView">
        <attr name="textSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="TypefaceTextView">
        <attr name="fontName">
            <enum name="DosisRegular" value="0"/>
            <enum name="DosisMedium" value="1"/>
            <enum name="DosisSemiBold" value="2"/>
            <enum name="DosisBold" value="3"/>
            <enum name="InconsolataRegular" value="4"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="BinaryTextureView">
        <attr name="backgroundColor" format="color"/>
    </declare-styleable>
</resources>
